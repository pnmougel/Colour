      <script type="text/javascript" language="javascript">
    var request_paras = {};    
    var pageurl_ranklist = window.location.toString();
    var question_mark_ranklist = pageurl_ranklist.lastIndexOf('?');
    (function () { request_paras = queryToJson(pageurl_ranklist); })();
    if (question_mark_ranklist > -1) {
        pageurl_ranklist = pageurl_ranklist.substr(0, question_mark_ranklist);
    }
    var s = [""];
    s.push(pageurl_ranklist);
    s.push("?");    
            
    function GetDomainID(para) {
        var domainID;
        try {
            domainID = decodeURI(request(para));
        }
        catch (ex) {
            domainID = "";
        }
        return domainID;
    }

    var ChangingWindowLocation = false;
    function CheckChangingWindowLocation() {
        if (ChangingWindowLocation == false) {
            ChangingWindowLocation = true;
            return true;
        }
        return false;
    }
</script> 
      <div id="ctl00_MainContent_RankListSummary_sriptSubDomain"> 
       <script type="text/javascript" language="javascript">
    //<![CDATA[

    function BindSubDomainFilter() {
        //author
        var data = { 'inputId': '#ctl00_MainContent_RankListSummary_drpSubDomain_drpList',
            "hiddenId": '#ctl00_MainContent_RankListSummary_drpSubDomain_drp_hidden_focus',
            'textId': '#ctl00_MainContent_RankListSummary_drpSubDomain_spSelectedText',
            'tsId': '#ctl00_MainContent_RankListSummary_drpSubDomain_drpListContent',
            'postAction': function () {
                if(CheckChangingWindowLocation() == false)
                {
                    return;
                }
                var value = $(DropDown.tsId + ' .selected').find(':hidden').text();

                var query = window.location.href;
                if (query) {
                    removeStartEndIndex(query);                    
                    if( typeof value != "undefined"){
                        request_paras.subdomainid = value;
                        s.push(jsonToQuery(request_paras));
                        window.location = s.join('');

                    }

                }
            }
        };
        DropDown.Init(data);
        var options = new Array();
        
        options[0] = { "Text": "Anthropology", "Value": "1"};options[1] = { "Text": "Communication", "Value": "2"};options[2] = { "Text": "Education", "Value": "13"};options[3] = { "Text": "Ethnology", "Value": "4"};options[4] = { "Text": "Geography", "Value": "5"};options[5] = { "Text": "Gerontology & Demography", "Value": "6"};options[6] = { "Text": "Law & Criminology", "Value": "3"};options[7] = { "Text": "Library Science", "Value": "12"};options[8] = { "Text": "Management", "Value": "14"};options[9] = { "Text": "Methodology of Social Science", "Value": "7"};options[10] = { "Text": "Political Science", "Value": "8"};options[11] = { "Text": "Public Affairs", "Value": "9"};options[12] = { "Text": "Social Issue", "Value": "11"};options[13] = { "Text": "Sociology", "Value": "10"};options[14] = { "Text": "Overall for Social Science", "Value": "0", "Selected": "true"};
       
        DropDown.SetOptions(options);
        //var domainID = GetDomainID("subDomainID");
        var domainID = "";
        if(typeof (request_paras.subdomainid) != "undefined"){
            domainID = request_paras.subdomainid;
        }
        if (domainID && domainID != "") {
            DropDown.SetDefaultValue(domainID);
        }
    }
    BindSubDomainFilter();
       //]]>
</script> 
