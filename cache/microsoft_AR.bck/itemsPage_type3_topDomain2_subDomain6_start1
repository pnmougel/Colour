       <script type="text/javascript" language="javascript">
    //<![CDATA[

    function BindSubDomainFilter() {
        //author
        var data = { 'inputId': '#ctl00_MainContent_RankListSummary_drpSubDomain_drpList',
            "hiddenId": '#ctl00_MainContent_RankListSummary_drpSubDomain_drp_hidden_focus',
            'textId': '#ctl00_MainContent_RankListSummary_drpSubDomain_spSelectedText',
            'tsId': '#ctl00_MainContent_RankListSummary_drpSubDomain_drpListContent',
            'postAction': function () {
                if(CheckChangingWindowLocation() == false)
                {
                    return;
                }
                var value = $(DropDown.tsId + ' .selected').find(':hidden').text();

                var query = window.location.href;
                if (query) {
                    removeStartEndIndex(query);                    
                    if( typeof value != "undefined"){
                        request_paras.subdomainid = value;
                        s.push(jsonToQuery(request_paras));
                        window.location = s.join('');

                    }

                }
            }
        };
        DropDown.Init(data);
        var options = new Array();
        
        options[0] = { "Text": "Algorithms & Theory", "Value": "1"};options[1] = { "Text": "Artificial Intelligence", "Value": "5"};options[2] = { "Text": "Bioinformatics & Computational Biology", "Value": "21"};options[3] = { "Text": "Computer Education", "Value": "23"};options[4] = { "Text": "Computer Vision", "Value": "11"};options[5] = { "Text": "Data Mining", "Value": "7"};options[6] = { "Text": "Databases", "Value": "18"};options[7] = { "Text": "Distributed & Parallel Computing", "Value": "16"};options[8] = { "Text": "Graphics", "Value": "10"};options[9] = { "Text": "Hardware & Architecture", "Value": "3"};options[10] = { "Text": "Human-Computer Interaction", "Value": "12"};options[11] = { "Text": "Information Retrieval", "Value": "8"};options[12] = { "Text": "Machine Learning & Pattern Recognition", "Value": "6"};options[13] = { "Text": "Multimedia", "Value": "13"};options[14] = { "Text": "Natural Language & Speech", "Value": "9"};options[15] = { "Text": "Networks & Communications", "Value": "14"};options[16] = { "Text": "Operating Systems", "Value": "17"};options[17] = { "Text": "Programming Languages", "Value": "24"};options[18] = { "Text": "Real-Time & Embedded Systems", "Value": "19"};options[19] = { "Text": "Scientific Computing", "Value": "22"};options[20] = { "Text": "Security & Privacy", "Value": "2"};options[21] = { "Text": "Simulation", "Value": "20"};options[22] = { "Text": "Software Engineering", "Value": "4"};options[23] = { "Text": "World Wide Web", "Value": "15"};options[24] = { "Text": "Overall for Computer Science", "Value": "0", "Selected": "true"};
       
        DropDown.SetOptions(options);
        //var domainID = GetDomainID("subDomainID");
        var domainID = "";
        if(typeof (request_paras.subdomainid) != "undefined"){
            domainID = request_paras.subdomainid;
        }
        if (domainID && domainID != "") {
            DropDown.SetDefaultValue(domainID);
        }
    }
    BindSubDomainFilter();
       //]]>
</script> 
