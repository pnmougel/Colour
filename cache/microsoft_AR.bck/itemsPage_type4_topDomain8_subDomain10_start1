                            //<![CDATA[
                            function BindRankTypeFilter() {
                                //year
                                var data = { 'inputId': '#ctl00_MainContent_drpRankType_drpList',
                                    "hiddenId": '#ctl00_MainContent_drpRankType_drp_hidden_focus',
                                    'textId': '#ctl00_MainContent_drpRankType_spSelectedText',
                                    'tsId': '#ctl00_MainContent_drpRankType_drpListContent',
                                    'postAction': function () {
                                        if(CheckChangingWindowLocation() == false)
                                        {
                                            return;
                                        }

                                        var value = $(DropDown.tsId + ' .selected').find(':hidden').text();
                                        var query = window.location.href;
                                        if (query) {
                                            removeStartEndIndex(query);
                                            $('#ctl00_MainContent_thCitation').attr('title', '');
                                            request_paras.orderby = value;
                                            s.push(jsonToQuery(request_paras));
                                            pageurl_ranklist = s.join('');     
                                            window.location = pageurl_ranklist;                    
                                        }
                                    }
                                };
                                DropDown.Init(data);
                                $('#ctl00_MainContent_drpRankType_drpListContent').width(80);
                                var options = new Array();
                                options[0] = { "Text": "H-Index", "Value": "6", "Selected": "true"};options[1] = { "Text": "Citation", "Value": "1"};


                                DropDown.SetOptions(options);
                                if( typeof (request_paras.last) != "null" && typeof(request_paras.last) != "undefined"){
                                    DropDown.SetDefaultValue(request_paras.orderby);
                                }
                            }
                            BindRankTypeFilter();

                           
