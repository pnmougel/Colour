       <script type="text/javascript" language="javascript">
    //<![CDATA[

    function BindSubDomainFilter() {
        //author
        var data = { 'inputId': '#ctl00_MainContent_RankListSummary_drpSubDomain_drpList',
            "hiddenId": '#ctl00_MainContent_RankListSummary_drpSubDomain_drp_hidden_focus',
            'textId': '#ctl00_MainContent_RankListSummary_drpSubDomain_spSelectedText',
            'tsId': '#ctl00_MainContent_RankListSummary_drpSubDomain_drpListContent',
            'postAction': function () {
                if(CheckChangingWindowLocation() == false)
                {
                    return;
                }
                var value = $(DropDown.tsId + ' .selected').find(':hidden').text();

                var query = window.location.href;
                if (query) {
                    removeStartEndIndex(query);                    
                    if( typeof value != "undefined"){
                        request_paras.subdomainid = value;
                        s.push(jsonToQuery(request_paras));
                        window.location = s.join('');

                    }

                }
            }
        };
        DropDown.Init(data);
        var options = new Array();
        
        options[0] = { "Text": "Aeronautics & Aerospace Engineering", "Value": "1"};options[1] = { "Text": "Biomedical Engineering", "Value": "2"};options[2] = { "Text": "Chemical Engineering", "Value": "3"};options[3] = { "Text": "Civil Engineering", "Value": "4"};options[4] = { "Text": "Construction", "Value": "5"};options[5] = { "Text": "Electrical & Electronic Engineering", "Value": "6"};options[6] = { "Text": "Energy", "Value": "7"};options[7] = { "Text": "Industrial Engineering", "Value": "8"};options[8] = { "Text": "Manufacturing Technology", "Value": "9"};options[9] = { "Text": "Mechanical Engineering", "Value": "10"};options[10] = { "Text": "Mining Engineering", "Value": "11"};options[11] = { "Text": "Nanotechnology", "Value": "12"};options[12] = { "Text": "Nuclear Engineering", "Value": "13"};options[13] = { "Text": "Ocean Engineering", "Value": "14"};options[14] = { "Text": "Reliability & Risk Analysis", "Value": "15"};options[15] = { "Text": "Transportation Engineering", "Value": "16"};options[16] = { "Text": "Overall for Engineering", "Value": "0", "Selected": "true"};
       
        DropDown.SetOptions(options);
        //var domainID = GetDomainID("subDomainID");
        var domainID = "";
        if(typeof (request_paras.subdomainid) != "undefined"){
            domainID = request_paras.subdomainid;
        }
        if (domainID && domainID != "") {
            DropDown.SetDefaultValue(domainID);
        }
    }
    BindSubDomainFilter();
       //]]>
</script> 
