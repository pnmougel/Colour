      <script type="text/javascript" language="javascript">
    var request_paras = {};    
    var pageurl_ranklist = window.location.toString();
    var question_mark_ranklist = pageurl_ranklist.lastIndexOf('?');
    (function () { request_paras = queryToJson(pageurl_ranklist); })();
    if (question_mark_ranklist > -1) {
        pageurl_ranklist = pageurl_ranklist.substr(0, question_mark_ranklist);
    }
    var s = [""];
    s.push(pageurl_ranklist);
    s.push("?");    
            
    function GetDomainID(para) {
        var domainID;
        try {
            domainID = decodeURI(request(para));
        }
        catch (ex) {
            domainID = "";
        }
        return domainID;
    }

    var ChangingWindowLocation = false;
    function CheckChangingWindowLocation() {
        if (ChangingWindowLocation == false) {
            ChangingWindowLocation = true;
            return true;
        }
        return false;
    }
</script> 
