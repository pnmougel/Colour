      <script type="text/javascript" language="javascript">
    //<![CDATA[
    function BindYearFilter() {
        //year
        var data = { 'inputId': '#ctl00_MainContent_RankListSummary_drpListYear_drpList',
            "hiddenId": '#ctl00_MainContent_RankListSummary_drpListYear_drp_hidden_focus',
            'textId': '#ctl00_MainContent_RankListSummary_drpListYear_spSelectedText',
            'tsId': '#ctl00_MainContent_RankListSummary_drpListYear_drpListContent',
            'postAction': function () {
                if(CheckChangingWindowLocation() == false)
                {
                    return;
                }

                var value = $(DropDown.tsId + ' .selected').find(':hidden').text();
                var query = window.location.href;
                if (query) {
                    removeStartEndIndex(query);

                    if( /\/detail[\.\?]/g.test(query.toLowerCase())){                        
                        // if this control is used by organization detail page                        

                        if(typeof value == "undefined") return;
                        if(typeof (request_paras.last) != "null" && typeof (request_paras.last) != "undefined" && value == 0 ){
                            delete request_paras.last;
                        }
                        if(value > 0){
                            request_paras.last = value;
                        }
                        s.push(jsonToQuery(request_paras));
                        pageurl_ranklist = s.join('');     
                        consolelog(pageurl_ranklist);
                        window.location = pageurl_ranklist;
                        return;
                    }                    
                    request_paras.last = value;
                    s.push(jsonToQuery(request_paras));
                    pageurl_ranklist = s.join('');     
                    consolelog(pageurl_ranklist);                      
                    window.location = pageurl_ranklist;                    

                }
            }
        };
        DropDown.Init(data);
        var options = new Array();
        options[0] = { "Text": "Last 5 Years", "Value": "5" };
        options[1] = { "Text": "Last 10 Years", "Value": "10" };
        options[2] = { "Text": "All Years", "Value": "0", "Selected": "true" };


        DropDown.SetOptions(options);
        if( typeof (request_paras.last) != "null" && typeof(request_paras.last) != "undefined"){
            DropDown.SetDefaultValue(request_paras.last);
        }
    }
    BindYearFilter();

    function removeStartEndIndex(query) {        
        if(typeof (request_paras.start) != "null" && typeof (request_paras.start) != "undefined"){        
            delete request_paras.start;
        }
        if(typeof (request_paras.end) != "null" && typeof (request_paras.end) != "undefined"){
            delete request_paras.end;
        }
        return true;      
    }
    function removeContinent(query) {        
        if(typeof (request_paras.continentid) != "null" && typeof (request_paras.continentid) != "undefined"){        
            delete request_paras.continentid;
        }
        return query;        
    }
    function GetLastYear() {
        var lastYear;
        try {
            lastYear = decodeURI(request("last"));
        }
        catch (ex) {
            lastYear = "";
        }

        return lastYear;
    }
    //]]>

    //<![CDATA[
    function BindTopDomainFilter() {
        //author
        var data = { 'inputId': '#ctl00_MainContent_RankListSummary_drpTopDomain_drpList',
            "hiddenId": '#ctl00_MainContent_RankListSummary_drpTopDomain_drp_hidden_focus',
            'textId': '#ctl00_MainContent_RankListSummary_drpTopDomain_spSelectedText',
            'tsId': '#ctl00_MainContent_RankListSummary_drpTopDomain_drpListContent',
            'postAction': function () {
                if(CheckChangingWindowLocation() == false)
                {
                    return;
                }
                var value = $(DropDown.tsId + ' .selected').find(':hidden').text();

                var query = window.location.href;
                
                //set subDomain to 0
                if (query) {
                    if(typeof (request_paras.subdomainid) != "null" && typeof (request_paras.subdomainid) != "undefined"){
                        request_paras.subdomainid = 0;
                    }
                    removeStartEndIndex(query);
                    if(typeof value != "undefined"){
                        request_paras.topdomainid = value;
                        s.push(jsonToQuery(request_paras));                        
                        window.location = s.join('');
                    }
                    


                    /*var regExp = new RegExp("&subDomainID=\\d+", "g");
                    var match = regExp.exec(query);
                    if (match && match.length == 1) {
                        query = query.replace(match, "&subDomainID=0");
                    }

                    query = removeStartEndIndex(query);
                    regExp = new RegExp("topDomainID=\\d+", "g");
                    match = regExp.exec(query);
                    if (match && match.length == 1) {
                        query = query.replace(match, "topDomainID=" + value);
                        window.location = query;
                    }*/
                }                
            }
        };
        DropDown.Init(data);
        var options = new Array();
        
        options[0] = { "Text": "Agriculture Science", "Value": "16"};options[1] = { "Text": "Arts & Humanities", "Value": "3"};options[2] = { "Text": "Biology", "Value": "4"};options[3] = { "Text": "Chemistry", "Value": "5"};options[4] = { "Text": "Computer Science", "Value": "2"};options[5] = { "Text": "Economics & Business", "Value": "7"};options[6] = { "Text": "Engineering", "Value": "8"};options[7] = { "Text": "Environmental Sciences", "Value": "9"};options[8] = { "Text": "Geosciences", "Value": "10"};options[9] = { "Text": "Material Science", "Value": "12"};options[10] = { "Text": "Mathematics", "Value": "15"};options[11] = { "Text": "Medicine", "Value": "6"};options[12] = { "Text": "Physics", "Value": "19"};options[13] = { "Text": "Social Science", "Value": "22"};options[14] = { "Text": "Multidisciplinary", "Value": "1"};
       
        DropDown.SetOptions(options);
        var domainID = GetDomainID("topDomainID");
        if (domainID && domainID != "") {
            DropDown.SetDefaultValue(domainID);
        }else{
            if( options.length > 0  && options[options.length-1].Value == "0"){      
                var t = options[options.length-1].Value;                
                DropDown.SetDefaultValue(t);
            }else{
                DropDown.SetDefaultValue();
            }
        }
    }
    BindTopDomainFilter();
    //]]>
</script> 
