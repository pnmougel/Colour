       <script type="text/javascript" language="javascript">
    //<![CDATA[

    function BindSubDomainFilter() {
        //author
        var data = { 'inputId': '#ctl00_MainContent_RankListSummary_drpSubDomain_drpList',
            "hiddenId": '#ctl00_MainContent_RankListSummary_drpSubDomain_drp_hidden_focus',
            'textId': '#ctl00_MainContent_RankListSummary_drpSubDomain_spSelectedText',
            'tsId': '#ctl00_MainContent_RankListSummary_drpSubDomain_drpListContent',
            'postAction': function () {
                if(CheckChangingWindowLocation() == false)
                {
                    return;
                }
                var value = $(DropDown.tsId + ' .selected').find(':hidden').text();

                var query = window.location.href;
                if (query) {
                    removeStartEndIndex(query);                    
                    if( typeof value != "undefined"){
                        request_paras.subdomainid = value;
                        s.push(jsonToQuery(request_paras));
                        window.location = s.join('');

                    }

                }
            }
        };
        DropDown.Init(data);
        var options = new Array();
        
        options[0] = { "Text": "Anatomy", "Value": "1"};options[1] = { "Text": "Andrology", "Value": "2"};options[2] = { "Text": "Cardiology", "Value": "3"};options[3] = { "Text": "Dentistry", "Value": "4"};options[4] = { "Text": "Dermatology", "Value": "5"};options[5] = { "Text": "Diabetes", "Value": "6"};options[6] = { "Text": "Diseases", "Value": "7"};options[7] = { "Text": "Emergency & Critical Care", "Value": "8"};options[8] = { "Text": "Endocrinology", "Value": "9"};options[9] = { "Text": "Family Medicine", "Value": "10"};options[10] = { "Text": "Gynecology & Obstetrics", "Value": "11"};options[11] = { "Text": "Immunology", "Value": "12"};options[12] = { "Text": "Medical Education & Training", "Value": "13"};options[13] = { "Text": "Neuroscience", "Value": "14"};options[14] = { "Text": "Nursing", "Value": "15"};options[15] = { "Text": "Nutrition", "Value": "16"};options[16] = { "Text": "Oncology", "Value": "17"};options[17] = { "Text": "Ophthalmology", "Value": "18"};options[18] = { "Text": "Pathology", "Value": "19"};options[19] = { "Text": "Pharmacology", "Value": "20"};options[20] = { "Text": "Physiology", "Value": "21"};options[21] = { "Text": "Psychiatry & Psychology", "Value": "22"};options[22] = { "Text": "Urology", "Value": "23"};options[23] = { "Text": "Overall for Medicine", "Value": "0", "Selected": "true"};
       
        DropDown.SetOptions(options);
        //var domainID = GetDomainID("subDomainID");
        var domainID = "";
        if(typeof (request_paras.subdomainid) != "undefined"){
            domainID = request_paras.subdomainid;
        }
        if (domainID && domainID != "") {
            DropDown.SetDefaultValue(domainID);
        }
    }
    BindSubDomainFilter();
       //]]>
